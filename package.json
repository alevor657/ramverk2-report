{
    "name": "ramverk2-report",
    "version": "1.0.0",
    "description": "A report page for course ramverk2",
    "main": "index.js",
    "scripts": {
        "webpack": "webpack",
        "start": "node app/server.js",
        "test": "npm run eslint && npm run stylelint && npm run jest",
        "stylelint": "stylelint \"**/*.scss\"",
        "eslint": "eslint .; exit 0",
        "jest": "jest --coverage",
        "coverage": "open('coverage/lcov-report/index.html')",
        "node_latest": "docker-compose up node_latest",
        "node_6": "docker-compose up node6",
        "node_8": "docker-compose up node8",
        "test_latest": "docker-compose run node_latest npm test",
        "test_6": "docker-compose run node6 npm test",
        "test_8": "docker-compose run node8 npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alevor657/ramverk2-report.git"
    },
    "author": "Alexey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alevor657/ramverk2-report/issues"
    },
    "homepage": "https://github.com/alevor657/ramverk2-report#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.7",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "eslint": "^4.10.0",
        "eslint-plugin-jest": "^21.3.2",
        "eslint-plugin-react": "^7.4.0",
        "file-loader": "^1.1.5",
        "jest": "^21.2.1",
        "node-sass": "^4.6.0",
        "normalize.css": "^7.0.0",
        "open": "0.0.5",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-test-renderer": "^16.1.1",
        "redux": "^3.7.2",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "stylelint": "^8.2.0",
        "stylelint-scss": "^2.1.0",
        "supertest": "^3.0.0",
        "webpack": "^3.8.1"
    },
    "dependencies": {
        "express": "^4.16.2",
        "prop-types": "^15.6.0",
        "react-router-dom": "^4.2.2",
        "socket.io": "^2.0.4"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
        }
    }
}
